<!DOCTYPE html>
<html>
<head>
  <title></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<br>  
<div class="well container-fluid col-xs-8 col-xs-offset-2">
  <h1>Admin</h1>
  <p class="text-right"><%= link_to("<< Back to Main", {:controller => 'admin', :action => 'index'}) %></p>

  <% flash.each do |key, value| %>
    <div class="alert alert-<%= key %>" role="alert">
      <%= value %>
    </div>
  <% end %>

  <%= yield %>

  <%#= params %>
</div>

</body>
</html>
